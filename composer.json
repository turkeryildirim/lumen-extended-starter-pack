{
    "name": "turkeryildirim/simple-lumen-boilerplate",
    "description": "Simple Lumen Boilerplate - A starter kit to build Lumen applications.",
    "keywords": ["framework", "laravel", "lumen", "boilerplate", "starter kit"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.*",
        "illuminate/notifications": "^5.6",
        "illuminate/mail": "^5.6",
        "illuminate/redis": "^5.6",
        "vlucas/phpdotenv": "^2.5",
        "guzzlehttp/guzzle": "^6.3",
        "zircote/swagger-php": "^3.0",
        "darkaonline/swagger-lume": "^5.6",
        "barryvdh/laravel-cors": "^0.11",
        "pda/pheanstalk": "^3.1",
        "flipbox/lumen-generator": "^5.6",
        "flugger/laravel-responder": "^3.0",
        "intervention/image": "^2.4",
        "turkeryildirim/lumen-config-autoloader": "^1.0",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "phpunit/phpunit": "^7.3",
        "mockery/mockery": "^1.1",
        "squizlabs/php_codesniffer": "^3.3",
        "barryvdh/laravel-ide-helper": "^2.4",
        "phpmd/phpmd": "^2.6",
        "phploc/phploc": "^4.0",
        "sebastian/phpcpd": "^4.0",
        "laravelista/lumen-vendor-publish": "^2.1",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/phpunit",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
